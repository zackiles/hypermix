{
  "deno.enable": true,
  "deno.config": "./deno.json",
  "deno.importMap": "./deno.json",
  "deno.unstable": [
    "net",
    "http",
    "fs",
  ],
  "deno.codeLens.implementations": true,
  "deno.codeLens.references": true,
  "deno.codeLens.referencesAllFunctions": true,
  "deno.codeLens.test": true,
  "deno.internalDebug": true,
  "deno.internalInspect": false,
  "deno.maxTsServerMemory": 6000,
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "editor.detectIndentation": false,
  "editor.indentSize": 2,
  "editor.insertSpaces": true,
  "workbench.editorAssociations": {
    "*.mdc": "default",
    "*.copilotmd": "vscode.markdown.preview.editor",
    "*.md": "vscode.markdown.preview.editor"
  },
  "files.associations": {
    "repomix.config.json": "json5"
  },
  "[json][jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown][typescript][javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "cSpell.dictionaries": [
    "!typescript"
  ],
  "cSpell.words": [
    "agentic",
    "anthropics",
    "asynciterable",
    "beforeunload",
    "cursorignore",
    "cursorignoreindex",
    "datacontenttype",
    "dataschema",
    "dbus",
    "dconf",
    "deno",
    "denoland",
    "dockerenv",
    "dotenv",
    "FOSS",
    "fseventsd",
    "hiberfil",
    "hypermix",
    "Hypermixing",
    "Iles",
    "inprocess",
    "keyrings",
    "kvdex",
    "longerfilename",
    "MAKECMDGOALS",
    "Makefiles",
    "MILLIS",
    "modelcontextprotocol",
    "msvc",
    "notexists",
    "olli",
    "onclose",
    "parentbased",
    "pids",
    "repomix",
    "repomixing",
    "Repoxmix",
    "rwxr",
    "somevalue",
    "specversion",
    "swapfile",
    "traceparent",
    "tracestate",
    "typeof",
    "unvalidated",
    "USERPROFILE",
    "valibot",
    "webgpu",
    "yieldray",
    "zacharyiles",
    "zackiles",
    "Zipkin"
  ]
}
